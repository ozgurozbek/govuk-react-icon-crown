{
  "name": "@govuk-react/icon-crown",
  "version": "0.0.8",
  "description": "GOV.UK Crown icon as a React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "source": "src/index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:es",
    "build:lib": "rimraf lib && babel src -d lib --source-maps",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src -d es --source-maps"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.10",
    "@babel/plugin-transform-runtime": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@babel/preset-react": "7.16.7",
    "babel-plugin-add-module-exports": "^1.0.4",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2"
  },
  "author": "Alasdair McLeay",
  "directories": {
    "lib": "lib"
  },
  "peerDependencies": {
    "react": ">=16.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penx/govuk-react-icon-crown.git"
  },
  "bugs": {
    "url": "https://github.com/penx/govuk-react-icon-crown/issues"
  },
  "homepage": "https://github.com/penx/govuk-react-icon-crown#readme"
}
